                                <TextBlock TextWrapping="Wrap" Text="Ürün Stok Kodu : K0004" Margin="22,787,73,0" FontSize="20" LineHeight="20" Grid.Column="1" Height="39" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
                                <TextBlock TextWrapping="Wrap" Text="Ürün Adı : Kavlak 2li Özel Fiyat Naturel Sızma Soğuk Sıkım Zeytinyağı 5 LT" Margin="22,835,73,0" FontSize="20" LineHeight="20" Grid.Column="1" Height="39" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
                                <TextBlock TextWrapping="Wrap" Text="Değerli Müşterilerimiz, filtrasyon evresinden geçmiş zeytinyağlarını dahi çalkalayarak tüketiniz." Margin="22,876,73,0" FontSize="20" LineHeight="20" Grid.Column="1" Height="70" VerticalAlignment="Top" FontStyle="Italic" Grid.ColumnSpan="2"/>
                                <TextBlock Text="   Teslimat Bilgileri" Margin="2,0,0,1206" FontSize="25" Background="#FFEDEDED" TextAlignment="Left"   LineHeight="10" HorizontalAlignment="Left" Width="1103" VerticalAlignment="Bottom" RenderTransformOrigin="0.5,0.5" Height="50" Grid.ColumnSpan="2" Grid.Column="1">
                                    <TextBlock.RenderTransform>
